{
  "name": "@bryce-loskie/tc",
  "version": "0.1.4",
  "description": "tsconfig.json base configs",
  "keywords": [
    "tsconfig",
    "tsconfig-base",
    "tsconfig.json",
    "typescript-config"
  ],
  "homepage": "https://github.com/guygubaby/tc#README",
  "repository": {
    "type": "git",
    "url": "https://github.com/guygubaby/tc"
  },
  "author": "guygubaby@gmail.com",
  "files": [
    "*.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./lib": {
      "default": "./lib.json"
    },
    "./lib-react": {
      "default": "./lib-react.json"
    },
    "./react": {
      "default": "./react.json"
    },
    "./vue": {
      "default": "./vue.json"
    }
  },
  "scripts": {
    "dev": "pnpm -r dev",
    "build": "pnpm -r build",
    "release": "bumpp package.json --commit --push && pnpm run push",
    "push": "npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "bumpp": "^8.2.1",
    "typescript": "4.9.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "packageManager": "pnpm@7.26.1"
}
